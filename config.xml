<?xml version='1.0' encoding='utf-8'?>
<widget id="no.byweb.ryzom" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="SplashScreen" value="splash" />
    <preference name="SplashScreenDelay" value="10000" />
    <name>Ryztools</name>
    <description>
        Ryztools - A collection of tools for the Ryzom MMORPG.
    </description>
    <author email="tux@krageroby.no" href="http://ryzom.byweb.no">
        Tux Solbakk (Drogos)
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
</widget>
